{
  "name": "alyou/belajar-php-unit-test-2",
  "description": "belajar php unit test",
  "type": "project",
  "autoload": {
    "psr-4": {
      "Alyou\\BelajarPhpUnitTest2\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml",
    "test:person": "phpunit --configuration phpunit.xml --testsuite person",
    "test:counter": "phpunit --configuration phpunit.xml --testsuite counter"
  },
  "autoload-dev": {
    "psr-4": {
      "Alyou\\BelajarPhpUnitTest2\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "alyouma",
      "email": "alyoumaakmal4@gmail.com"
    }
  ],
  "require": {},
  "require-dev": {
    "phpunit/phpunit": "9.5.5"
  }
}
